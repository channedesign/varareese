<div id="video">
	<div class="fifty marginTopMin">
		<iframe class="big_video" src="https://player.vimeo.com/video/123171770?title=0&byline=0&portrait=0" width="100%" frameborder="0" webkitallowfullscreen mozallowfullscreen allowfullscreen></iframe>
	</div>
	<div class="fifty center marginTop">
		<h1>Video</h1>
		<p class="lead">Feel free to have a look at my work</p>
	</div>
	<div class="anchor_video">
		<div class="video_carousel">
			<% @videos.each do |video| %>
				<div><%= video.link.html_safe %></div>
			<% end %>
		</div>
		<div class="left_arrow_pic_div arrow_pic_div">
			<%= image_tag("left_arrow.png", class: "arrow_pic", id: "left_arrow_pic") %>
		</div>
		<div class="right_arrow_pic_div arrow_pic_div">
			<%= image_tag("right_arrow.png", class: "arrow_pic", id: "right_arrow_pic") %>
		</div>
	</div>
</div>
