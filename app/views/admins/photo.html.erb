<%= render template: "shared/admins_header" %>
<div class="container">
	<div class="row center">
		<div class="col-md-6">
			<h3>Upload Your Photo</h3>
			<%= render template: "photos/new", class: "center"%>
		</div>
		<div class="col-md-6">
			<h3>Edit Your Photo</h3>
			<% @photos.each do |photo| %>
				<div class="col-sm-3 mini_thumb_div">
					<%= link_to(image_tag(photo.photo.url(:thumb), class: "mini_thumb"), edit_photo_path(photo)) %>
				</div>
			<% end %>
		</div>
	</div>
</div>
			
			